#include<iostream>
#include<stdio.h>
#include<stdlib.h>
#include<time.h>
//este programa genera una matriz 3x3 rellena de numeros enteros entre el 0 y el 9 con repetición
using namespace std;

int main(){
	long semillita;
	semillita=time(0);
	srand(semillita);
	cout<<"un número al azar: "<<rand();
	return 0;
}
